@OData.publish : true
context UserData {
    @Comment : 'User DB'
    entity User {
            @Comment : 'User ID'
        key UserId    : Integer generated always as identity(start with 1000000225 increment by 1 no minvalue maxvalue 1999999999 no cache no cycle);

            @Comment : 'First Name'
            FirstName : String(40);

            @Comment : 'Last Name'
            LastName  : String(40);

            @Comment : 'Email'
            Email     : String(255);
    };

    @Comment : 'User DB'
    entity User2 {
            @Comment : 'User ID'
        key UserId    : Integer generated always as identity(start with 1000000225 increment by 1 no minvalue maxvalue 1999999999 no cache no cycle);

            @Comment : 'First Name'
            FirstName : String(40);

            @Comment : 'Last Name'
            LastName  : String(40);

            @Comment : 'Email'
            Email     : String(255);
    };
};